{
  "name": "ember-web-app",
  "version": "2.3.0",
  "description": "This Ember addon helps you configure and manage the web app manifest and related meta tags needed to create a Progressive Web Application",
  "keywords": [
    "PWA",
    "android",
    "ember-addon",
    "ios",
    "iphone",
    "manifest.json",
    "progressive web applications"
  ],
  "license": "MIT",
  "author": "Santiago Ferreira",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/san650/ember-web-app",
  "scripts": {
    "build": "ember build",
    "fast-test": "SKIP_ACCEPTANCE=true npm run node-test",
    "node-test": "mocha --recursive node-tests",
    "start": "ember serve",
    "test": "ember try:each"
  },
  "dependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-merge-trees": "^2.0.0",
    "ember-cli-babel": "^6.6.0",
    "object-assign": "^4.1.1",
    "web-app-manifest-validator": "^1.0.0",
    "xmlbuilder": "^9.0.4"
  },
  "devDependencies": {
    "broccoli-test-helpers": "^0.0.9",
    "ember-ajax": "^3.0.0",
    "ember-cli": "2.17.0",
    "ember-cli-addon-docs": "^0.3.0",
    "ember-cli-addon-docs-yuidoc": "^0.1.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-blueprint-test-helpers": "^0.18.3",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.2",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-internal-test-helpers": "^0.9.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.17.0",
    "loader.js": "^4.2.3",
    "mocha": "^3.5.3",
    "rsvp": "^3.6.2",
    "strip-ansi": "^3.0.1"
  },
  "engines": {
    "node": "6.* || >= 7.* || >= 8.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "broccoli-asset-rev"
  },
  "homepage": "https://san650.github.io/ember-web-app"
}
